## Created by Anjuta


uidir = $(pkgdatadir)/ui
ui_DATA = g31_mixbox.ui


AM_CPPFLAGS = \
	-DPACKAGE_LOCALE_DIR=\""$(localedir)"\" \
	-DPACKAGE_SRC_DIR=\""$(srcdir)"\" \
	-DPACKAGE_DATA_DIR=\""$(pkgdatadir)"\" \
	$(G31_MIXBOX_CFLAGS)

AM_CFLAGS =\
	 -Wall\
	 -g

bin_PROGRAMS = g31_mixbox

g31_mixbox_SOURCES = \
	main.c \
	g31-mixbox.h \
	g31-mixbox.c	


g31_mixbox_LDFLAGS = \
	-Wl,--export-dynamic

g31_mixbox_LDADD = $(G31_MIXBOX_LIBS)




EXTRA_DIST = $(ui_DATA)

# Remove ui directory on uninstall
uninstall-local:
	-rm -r $(uidir)
	-rm -r $(pkgdatadir)

