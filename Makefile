.PHONY: clean All

All:
	@echo "----------Building project:[ 1-compile_hello - Debug ]----------"
	@cd "1-compile_hello" && "$(MAKE)" -f  "1-compile_hello.mk"
clean:
	@echo "----------Cleaning project:[ 1-compile_hello - Debug ]----------"
	@cd "1-compile_hello" && "$(MAKE)" -f  "1-compile_hello.mk" clean
